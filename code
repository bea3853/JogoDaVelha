algoritmo "JogodaVelha"
var
   a:vetor [1..3,1..3] de caracter
   l,c: inteiro
   vencedor: caracter
   resp:caracter
inicio

   escreval ("JOGO DA VELHA ")
   enquanto resp<>"n"faca
   para l de 1 ate 3 faca
      para c de 1 ate 3 faca
         escreva (l, "ยบ Rodada")
         escreva (c, "ยบ jogador - ")
         leia (a[l,c])
      fimpara

   fimpara

   para l de 1 ate 3 faca
      para c de  1 ate  3 faca
         escreva (a[l,c]:4)
      fimpara
      escreval()
   fimpara

   escreval("Digite o nome do vencedor")
   leia (vencedor)


      escreval ("Deseja repetir?")
       leia (resp)

 fimenquanto
fimalgoritmo
